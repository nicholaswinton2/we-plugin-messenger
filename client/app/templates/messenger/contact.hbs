<li {{bind-attr class=contactClass}}>
  <a {{action "startTalk" this}} >
    {{!-- <span class="avatar-small">{{we-avatar userId=contactUser.id}}</span> --}}
    <span class="avatar-small">{{user-avatar user=this}}</span>
    {{#if isOnline}}
      <span class="contact-status"><img src="/core/images/connected.png"></span>
    {{/if}}
    {{displayName}}
  </a>
</li>